<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TodaywordController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Suizlet_Background.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="114.0" fitWidth="218.0" layoutX="37.0" layoutY="36.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Suizlet%20LOGO.png" />
         </image>
      </ImageView>
      <ListView fx:id="EngList" layoutX="43.0" layoutY="190.0" prefHeight="448.0" prefWidth="200.0" />
      <ListView fx:id="KorList" layoutX="254.0" layoutY="190.0" prefHeight="448.0" prefWidth="189.0" />
      <Text fill="#09cf7e" layoutX="84.0" layoutY="167.0" stroke="WHITE" strokeType="OUTSIDE" strokeWidth="4.0" text="English">
         <font>
            <Font name="Franklin Gothic Heavy" size="34.0" />
         </font></Text>
      <Text fill="#09cf7e" layoutX="290.0" layoutY="168.0" stroke="WHITE" strokeType="OUTSIDE" strokeWidth="4.0" text="Korean">
         <font>
            <Font name="Franklin Gothic Heavy" size="34.0" />
         </font>
      </Text>
      <Button fx:id="LoadALL" layoutX="270.0" layoutY="60.0" mnemonicParsing="false" onAction="#LoadALL" prefHeight="59.0" prefWidth="70.0" styleClass="load" stylesheets="@style.css" text="LOAD">
         <font>
            <Font name="Franklin Gothic Demi" size="12.0" />
         </font>
      </Button>
      <Button fx:id="EngOnly" layoutX="348.0" layoutY="61.0" mnemonicParsing="false" onAction="#EngOnly" styleClass="only" stylesheets="@style.css" text="English Only">
         <font>
            <Font name="Franklin Gothic Heavy" size="12.0" />
         </font>
      </Button>
      <Button fx:id="KorOnly" layoutX="348.0" layoutY="92.0" mnemonicParsing="false" onAction="#KorOnly" styleClass="only" stylesheets="@style.css" text="Korean Only">
         <font>
            <Font name="Franklin Gothic Heavy" size="12.0" />
         </font>
      </Button>
      <Button fx:id="home" layoutX="222.0" layoutY="658.0" mnemonicParsing="false" onAction="#buttonClick" styleClass="only" stylesheets="@style.css" text="HOME">
         <font>
            <Font name="Franklin Gothic Heavy" size="12.0" />
         </font>
      </Button>
   </children>
</Pane>
